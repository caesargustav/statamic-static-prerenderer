{
    "name": "caesargustav/statamic-static-prerenderer",
    "autoload": {
        "psr-4": {
            "Caesargustav\\StaticPrerenderer\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Static Prerenderer",
            "description": "A Statamic addon that prerenders your site to static HTML files which can be fetched via an API."
        },
        "laravel": {
            "providers": [
                "Caesargustav\\StaticPrerenderer\\ServiceProvider"
            ]
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "statamic/cms": "^5.46",
        "laravel/pint": "^1.20"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
